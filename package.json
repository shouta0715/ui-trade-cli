{
  "name": "ui-trade",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "ui-trade": "./dist/server/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:client": "vite --port 3333",
    "dev:server": "rollup -c rollup.config.js --watch --bundleConfigAsCjs",
    "run:nodemon": "nodemon --watch dist/server/index.js --exec 'node dist/server/index.js preview'",
    "dev": "bun run:nodemon",
    "build": "rollup -c rollup.config.js --bundleConfigAsCjs && vite build",
    "prepare": "husky"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "chalk": "^5.3.0",
    "connect-history-api-fallback": "^2.0.0",
    "express": "^4.18.2",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.12.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "devDependencies": {
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.22",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.14.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --write"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
